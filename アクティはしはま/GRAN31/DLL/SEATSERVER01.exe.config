    <add key="SCLMENU_PATH" value="C:\Users\Administrator\Desktop\スクールメニュー" />
    <!-- 予約受講画面ファイルパス -->
    <add key="SCL010_PATH" value="C:\Users\Administrator\Desktop\予約受講画面" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Mono.Security" publicKeyToken="0738EB9F132ED756" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>