    <!-- ICR700リーダライターCOMポート名 -->
    <add key="ICR700RW_COM" value="COM1" />
    <!-- 通信変換機COMポート名 -->
    <add key="SEAT_COM" value="COM8" />
    <!-- データベース接続情報ファイルパス -->
    <add key="DbInfo_PATH" value="D:\GRAN31\DLL\DbInfo.xml" />
